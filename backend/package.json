  },
  "dependencies": {
    "express": "^5.1.0",
    "@supabase/supabase-js": "^2.76.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "zod": "^3.23.8",
    "winston": "^3.14.2",
    "langchain": "^1.0.1",
    "@langchain/core": "^1.0.0",
    "@langchain/google-genai": "^1.0.0",
    "@langchain/community": "^1.0.0",
    "@pinecone-database/pinecone": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.8.6",
    "@types/express": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/bcryptjs": "^2.4.6",
    "typescript": "^5.6.3",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.13",
    "ts-jest": "^29.2.5",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.13.0",
    "@eslint/js": "^9.13.0",
    "@typescript-eslint/parser": "^8.0.0",
